{
  "name": "diaz-on-demand",
  "private": true,
  "version": "0.1.0",
  "packageManager": "pnpm@9.12.3",
  "scripts": {
    "dev": "turbo run dev --filter=api --filter=diaz-ondemand-web --filter=@diaz/shared --filter=@diaz/db --filter=@diaz/ui",
    "dev:mobile": "pnpm --filter mobile dev",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "typecheck": "turbo run typecheck",
    "test": "turbo run test",
    "db:generate": "pnpm --filter @diaz/db db:generate",
    "db:migrate": "pnpm --filter @diaz/db db:migrate",
    "db:seed": "pnpm --filter @diaz/db db:seed"
  },
  "devDependencies": {
    "@types/node": "^22.13.4",
    "eslint": "^8.57.1",
    "prettier": "^3.5.0",
    "turbo": "^2.4.2",
    "typescript": "^5.7.3"
  }
}
